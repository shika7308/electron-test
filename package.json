  "build": {
    "appId": "test appId",
    "copyright": "test copyright",
    "asar": true,
    "productName": "test productName",
    "files": [
      "src/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ],
      "artifactName": "${name}-installer-${version}-${os}.${ext}"
    },
    "mac": {
      "artifactName": "${name}-installer-${version}-${os}.${ext}"
    },
    "win": {
      "certificateFile": "../../Desktop/TempSPC.pfx",
      "certificatePassword": "test"
    },
    "nsis": {
      "artifactName": "${name}-installer-${version}-${os}.${ext}"
    },
    "linux": {
      "artifactName": "${name}-installer-${version}-${os}.${ext}"
    }
  }
