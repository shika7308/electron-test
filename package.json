{
  "name": "electron-test",
  "version": "1.0.1",
  "description": "test for electron builder.",
  "main": "src/main.js",
  "scripts": {
    "dist-win": "build --win --x64",
    "dist-mac": "build --mac",
    "dist-linux": "build --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "",
  "license": "-",
  "dependencies": {
    "electron-config": "^1.0.0",
    "electron-log": "^2.2.14",
